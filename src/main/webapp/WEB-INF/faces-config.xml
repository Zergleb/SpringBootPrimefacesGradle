<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        version="2.2">
    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    <converter>
        <converter-id>integerConverter</converter-id>
        <converter-class>com.anderl.helper.IntegerConverter</converter-class>
    </converter>
    <converter>
        <converter-id>longConverter</converter-id>
        <converter-class>com.anderl.helper.LongConverter</converter-class>
    </converter>
</faces-config>